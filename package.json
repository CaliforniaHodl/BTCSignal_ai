{
  "name": "btcsignal-ai",
  "version": "1.0.0",
  "description": "AI-powered Bitcoin trading signals and market analysis platform",
  "scripts": {
    "build": "hugo --gc --minify",
    "test": "cypress run",
    "test:open": "cypress open",
    "test:ci": "cypress run --browser chrome",
    "test:e2e": "cypress run --spec 'cypress/e2e/**/*'",
    "test:api": "cypress run --spec 'cypress/api/**/*'",
    "test:unit": "cypress run --spec 'cypress/unit/**/*'",
    "test:visual": "cypress run --spec 'cypress/visual/**/*'",
    "sass:build": "sass assets/src/scss:static/src/css --style=compressed --no-source-map",
    "sass:watch": "sass assets/src/scss:static/src/css --watch"
  },
  "dependencies": {
    "@netlify/blobs": "^8.1.0",
    "@netlify/functions": "^2.4.0",
    "@octokit/rest": "^20.0.0",
    "@sparticuz/chromium": "^143.0.0",
    "axios": "^1.13.2",
    "canvas": "^3.2.0",
    "dotenv": "^17.2.3",
    "hugo-extended": "^0.139.3",
    "puppeteer-core": "^24.32.0",
    "socks-proxy-agent": "^8.0.5",
    "technicalindicators": "^3.1.0",
    "twitter-api-v2": "^1.28.0"
  },
  "devDependencies": {
    "cypress": "^15.7.1",
    "sass": "^1.94.2",
    "typescript": "^5.9.3"
  }
}
